;; Settings changed via Emacs customization menus

;; NOTE: use command 'describe-face' with cursor over text to view the face name
(custom-set-faces
 ;; custom-set-faces was added by Custom.
 ;; If you edit it by hand, you could mess it up, so be careful.
 ;; Your init file should contain only one such instance.
 ;; If there is more than one, they won't work right.
 '(org-code ((t (:foreground "red"))))
 '(org-done ((t (:foreground "forest green" :weight bold))))
 '(org-level-1 ((t (:inherit default))))
 '(org-level-2 ((t (:inherit default :weight normal :height 1.0))))
 '(org-level-3 ((t (:inherit default :weight normal :height 1.0))))
 '(org-level-4 ((t (:inherit default :weight normal :height 1.0))))
 '(org-link ((t (:foreground "deep sky blue" :box (:line-width 2 :color "deep sky blue" :style released-button)))))
 '(org-verbatim ((t (:foreground "firebrick3"))))
 '(markdown-header-face-1 ((t (:inherit org-level-1))))
 '(markdown-header-face-2 ((t (:inherit org-level-2))))
 '(markdown-header-face-3 ((t (:inherit org-level-3))))
 '(markdown-header-face-4 ((t (:inherit org-level-4))))
 '(markdown-header-face-5 ((t (:inherit org-level-5)))))

(custom-set-variables
 ;; custom-set-variables was added by Custom.
 ;; If you edit it by hand, you could mess it up, so be careful.
 ;; Your init file should contain only one such instance.
 ;; If there is more than one, they won't work right.
 '(whitespace-style (quote (face trailing tabs)))
 '(electric-indent-mode nil)
 '(markdown-fontify-code-blocks-natively t))
